

<!-- Bu kısmi görünüm sayfası -->


@model dynamic

<div class="card">
    <img src="@Model.ImageUrl" alt="@Model.Name" />
    <h3>@Model.Name</h3>
    <p>@Model.Description</p>
    @if (Model.DetailsUrl != null)
    {
        <button onclick="location.href='@Model.DetailsUrl'">
            Learn More
        </button>
    }
</div>

